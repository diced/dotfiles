WARP=$(curl https://cloudflare.com/cdn-cgi/trace --silent | sed -En 's/warp=(on|off)/\1/gp')

if [ "$WARP" = "off" ]; then
  sudo wg-quick up .config/wgcf/wgcf-profile.conf 2> /dev/null
  echo "WARP is on";
  dunstify -i oneoneoneone WARP on;
else
  sudo wg-quick down .config/wgcf/wgcf-profile.conf 2> /dev/null
  echo "WARP is off";
  dunstify -i oneoneoneone WARP off;
fi;
